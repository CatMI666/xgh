<view class="albums">
    <block wx:for="{{albums}}" wx:key="id" wx:for-index="idx" wx:for-item="album">
        <view class="album">
            <view class="name">{{album.name}}</view>
            <view class="photo-total">{{album.photoTotal}}</view>
            <view class="update-time">{{album.formatUpdateTime}}</view>
        </view>
    </block>
</view>
